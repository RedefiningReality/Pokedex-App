							size: self.width, 24
						Color:
							rgba: 0, 0.8, 0, 1
						BorderImage:
							border: [int(min(self.width * (self.value / float(self.max)) if self.max else 0, 12))] * 4
							pos: self.x, self.center_y - 12
							size: self.width * (self.value / float(self.max)) if self.max else 0, 24
			Label:										#Moveset Label
				size_hint_y: None
				height: self.texture_size[1]
				text_size: self.width, None 
				text: 'Moveset:'
				font_name: 'Peepo'
				font_size: '20sp'
				color: 0, 0, 0, 1
			Label:										#Moveset
				id: moveset
				size_hint_y: None
				height: self.texture_size[1]
				text_size: self.width, None 
				text: 'Tackle\n   [Power 50, Accuracy 100, PP 35]\nGrowl\n   [Power 0, Accuracy 100, PP 40]'
				font_name: 'Peepo'
				font_size: '20sp'
				color: 0, 0, 0, 1
			Label:										#Evolutions Label
