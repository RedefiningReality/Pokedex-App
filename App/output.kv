<OutputScreen>:

	image: image
	pname: name
	description: description
	type: type
	strength: strength
	weakness: weakness
	abilities: abilities
	hp: hp
	attack: attack
	defense: defense
	sp_attack: sp_attack
	sp_defense: sp_defense
	speed: speed
	moveset: moveset
	evolutions: evolutions

	orientation: 'vertical'
	ScrollView:
		GridLayout:
			cols: 1
			height: self.minimum_height
			size_hint_y: None
			spacing: 5
			padding: 20
			Button:											#Back Button
				on_release:
					app.root.current = 'Start'
					root.manager.transition.direction = 'right'
				markup: True
				size_hint: None, None
				size: 40, 40
				background_color: 0, 0, 0, 0
				text: '[b]<[/b]'
				font_name: 'Peepo'
				font_size: '50sp'
				color: 0, 0, 0, 1
			Image:											#Image
				id: image
				size_hint_y: None
				source: 'Pokemon UI Images/#001 Bulbasaur.png'
				size: self.texture_size
			Label:											#Name
				id: name
				size_hint_y: None
				height: self.texture_size[1]
				text_size: self.width, None 
				text: '#001 Bulbasaur'
				font_name: 'Peepo'
				font_size: '30sp'
				color: 0, 0, 0, 1
				halign: 'center'
			Label:											#Description
				id: description
				size_hint_y: None
				height: self.texture_size[1]
				text_size: self.width, None 
				text: 'There is a plant seed on its back right from the day this Pokemon is born. The seed slowly grows larger.'
				font_name: 'Peepo'
				font_size: '20sp'
				color: 0, 0, 0, 1
			Label:											#Type Label
				size_hint_y: None
				height: self.texture_size[1]
				text_size: self.width, None 
				text: 'Type:'
				font_name: 'Peepo'
				font_size: '20sp'
				color: 0, 0, 0, 1
			Label:											#Type
				id: type
				markup: True
				size_hint_y: None
				height: self.texture_size[1]
				text_size: self.width, None 
				text: '   [color=#88cc1b]Grass[/color]' + '   ' + '[color=#c334eb]Poison[/color]'
				font_name: 'Peepo'
				font_size: '20sp'
			Label:											#Strength Label
				size_hint_y: None
				height: self.texture_size[1]
				text_size: self.width, None 
				text: 'Strength:'
				font_name: 'Peepo'
				font_size: '20sp'
				color: 0, 0, 0, 1
			Label:											#Strength 
				id: strength
				markup: True
				size_hint_y: None
				height: self.texture_size[1]
				text_size: self.width, None 
				text: '   [color=#ff3d51]Fire[/color]' + '   ' + '[color=#5c8ff7]Flying[/color]' + '   ' + '[color=#ffffff]Ice[/color]' + '   ' + '[color=#fc03e8]Psychic[/color]'
				font_name: 'Peepo'
				font_size: '20sp'
			Label:											#Weakness Label
				size_hint_y: None
				height: self.texture_size[1]
				text_size: self.width, None 
				text: 'Weakness:'
				font_name: 'Peepo'
				font_size: '20sp'
				color: 0, 0, 0, 1
			Label:											#Weakness 
				id: weakness
				markup: True
				size_hint_y: None
				height: self.texture_size[1]
				text_size: self.width, None 
				text: '   [color=#ff3d51]Fire[/color]' + '   ' + '[color=#5c8ff7]Flying[/color]' + '   ' + '[color=#ffffff]Ice[/color]' + '   ' + '[color=#fc03e8]Psychic[/color]'
				font_name: 'Peepo'
				font_size: '20sp'
			Label:											#Abilities Label
				size_hint_y: None
				height: self.texture_size[1]
				text_size: self.width, None 
				text: 'Abilities:'
				font_name: 'Peepo'
				font_size: '20sp'
				color: 0, 0, 0, 1
			Label:											#Abilities 
				id: abilities
				size_hint_y: None
				height: self.texture_size[1]
				text_size: self.width, None 
				text: '   Overgrow'
				font_name: 'Peepo'
				font_size: '20sp'
				color: 0, 0, 0, 1
			Label:											#Stats Label
				size_hint_y: None
				height: self.texture_size[1]
				text_size: self.width, None 
				text: 'Stats:'
				font_name: 'Peepo'
				font_size: '20sp'
				color: 0, 0, 0, 1
			GridLayout:
				cols: 2
				height: self.minimum_height
				size_hint_y: None
				spacing: 5
				Label:											#HP
					size_hint_y: None
					height: self.texture_size[1]
					text_size: self.width, None 
					text: 'HP 45'
					font_name: 'Peepo'
					font_size: '20sp'
					color: 0, 0, 0, 1
				ProgressBar:
					id: hp
					size_hint_y: None
					height: 30
					min: 0
					max: 1
					value: 0.25
					canvas:
						Color:
							rgba: 0, 0.85, 1, 1
						BorderImage:
							border: (12, 12, 12, 12)
							pos: self.x, self.center_y - 12
							size: self.width, 24
						Color:
							rgba: 0, 0.8, 0, 1
						BorderImage:
							border: [int(min(self.width * (self.value / float(self.max)) if self.max else 0, 12))] * 4
							pos: self.x, self.center_y - 12
							size: self.width * (self.value / float(self.max)) if self.max else 0, 24
				Label:											#Attack
					size_hint_y: None
					height: self.texture_size[1]
					text_size: self.width, None 
					text: 'Attack 49'
					font_name: 'Peepo'
					font_size: '20sp'
					color: 0, 0, 0, 1
				ProgressBar:
					id: attack
					size_hint_y: None
					height: 30
					min: 0
					max: 1
					value: 0.25
					canvas:
						Color:
							rgba: 0, 0.85, 1, 1
						BorderImage:
							border: (12, 12, 12, 12)
							pos: self.x, self.center_y - 12
							size: self.width, 24
						Color:
							rgba: 0, 0.8, 0, 1
						BorderImage:
							border: [int(min(self.width * (self.value / float(self.max)) if self.max else 0, 12))] * 4
							pos: self.x, self.center_y - 12
							size: self.width * (self.value / float(self.max)) if self.max else 0, 24
				Label:											#Defense
					size_hint_y: None
